{
  "name": "quanto-professionals",
  "productName": "Quanto Professionals",
  "version": "1.0.1",
  "description": "A modern, offline-first invoice management desktop application for service providers",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "release:patch": "[ \"$(git branch --show-current)\" = \"main\" ] && git pull origin main && npm version patch && git push && git push --tags || echo 'Error: Must be on main branch'",
    "release:minor": "[ \"$(git branch --show-current)\" = \"main\" ] && git pull origin main && npm version minor && git push && git push --tags || echo 'Error: Must be on main branch'",
    "release:major": "[ \"$(git branch --show-current)\" = \"main\" ] && git pull origin main && npm version major && git push && git push --tags || echo 'Error: Must be on main branch'"
  },
  "keywords": [
    "invoice",
    "invoicing",
    "billing",
    "gst",
    "accounting",
    "desktop",
    "electron",
    "offline",
    "india",
    "tax",
    "freelancer",
    "small-business"
  ],
  "author": {
    "name": "Madhur Goyal",
    "email": "hi@mgoyal.com"
  },
  "license": "MIT",
  "repository": "https://github.com/mgoyal98/quanto-professionals",
